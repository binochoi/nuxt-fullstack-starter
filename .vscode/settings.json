{
  "css.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "typescript.preferences.autoImportFileExcludePatterns": [
    "vue-router"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.tabSize": 2
  },
  "[vue]": {
    "editor.tabSize": 2
  },
  "editor.insertSpaces": true
}