{
  "name": "app",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "run-s prepare.before; if-env NODE_ENV=prod && run-s prepare.prod || run-s prepare.dev; run-s prepare.after",
    "prepare.before": "dotenv -- run-s prepare.before.*",
    "prepare.before.primevue": "npx tsx scripts/primevue-support-nuxt-4.ts",
    "prepare.before.cp-env": "rimraf .env; cp .env.example .env; cp .env.example .env.prod",
    "prepare.before.set-token": "dotenv -- printf \"INFISICAL_TOKEN=$BINOCORP_INFISICAL_TOKEN\n\" >> .env",
    "prepare.after": "dotenv -- run-s prepare.after.*",
    "prepare.after.notify": "echo end",
    "prepare.dev": "dotenv -- run-s prepare.dev.*",
    "prepare.dev.env.common": "dotenv -- infisical export --env=dev --path=/ >> .env",
    "prepare.dev.env.local": "dotenv -- infisical export --env=dev --path=/nuxt-fullstack-starter >> .env",
    "prepare.dev.mkcert": "mkcert localhost",
    "prepare.dev.nuxt": "dotenv -- nuxt prepare",
    "prepare.prod": "run-s prod.env.*",
    "prepare.prod.env.common": "dotenv -- infisical export --env=prod --path=/ >> .env.prod",
    "prepare.prod.env.local": "dotenv -- infisical export --env=prod --path=/nuxt-fullstack-starter >> .env.prod",
    "dev": "run-p typecheck dev.only",
    "dev.only": "dotenv -- nuxt dev",
    "build": "run-s typecheck build.*",
    "build.only": "dotenv -- nuxt build",
    "typecheck": "run-s prepare.nuxt typecheck.only",
    "typecheck.only": "dotenv -- nuxt typecheck --cwd=src --logLevel=info",
    "generate": "nuxt generate",
    "preview": "run-s preview.*",
    "preview.build": "dotenv -v ENV=preview -- nuxt build",
    "preview.only": "dotenv -- pnpm preview.only.raw",
    "preview.only.raw": "wrangler dev --port=$PORT --https-key-path=./localhost-key.pem --https-cert-path=./localhost.pem --local-protocol https",
    "deploy": "run-s prod.env deploy.*",
    "deploy.build": "dotenv -e .env.prod -- nuxt build",
    "deploy.only": "dotenv -e .env.prod -- wrangler deploy"
  },
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "@ionic/vue": "^8.6.5",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@pinia/colada": "^0.17.1",
    "@pinia/nuxt": "^0.11.2",
    "@primeuix/themes": "^1.2.1",
    "@primevue/forms": "^4.3.6",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.16",
    "@vant/nuxt": "1.0.7",
    "@vueuse/core": "^13.5.0",
    "@vueuse/nuxt": "^13.5.0",
    "api-to-rpc": "^0.1.1",
    "better-auth": "^1.3.2",
    "drizzle-orm": "^0.44.3",
    "nitro-cloudflare-dev": "^0.2.2",
    "nitro-rpc-definition": "^0.1.8",
    "nuxt": "^4.0.1",
    "pinia": "^3.0.3",
    "postgres": "^3.4.7",
    "primevue": "^4.3.6",
    "sass": "^1.89.2",
    "superjson": "^2.2.2",
    "tailwindcss": "^3",
    "tailwindcss-animate": "^1.0.7",
    "timeago.js": "^4.0.2",
    "vant": "^4.9.21",
    "vue": "^3.5.17",
    "vue-final-modal": "^4.5.5",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@primevue/nuxt-module": "^4.3.6",
    "@types/node": "^24.1.0",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "~0.31.4",
    "env-cmd": "^10.1.0",
    "rimraf": "^6.0.1",
    "wrangler": "^4.25.0"
  }
}