{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "run-s prepare.*",
    "prepare.nuxt": "env-cmd -f .dev.vars nuxt prepare",
    "prepare.cp-env": "copyfiles -s .dev.vars.example .dev.vars",
    "build": "env-cmd -f .dev.vars nuxt build",
    "dev": "env-cmd -f .dev.vars nuxt dev",
    "generate": "nuxt generate",
    "preview": "run-s build preview.only",
    "preview.only": "env-cmd -f .dev.vars wrangler dev --port=3300 --local-protocol https --https-key-path ./localhost-key.pem --https-cert-path ./localhost.pem",
    "deploy": "run-s build deploy.only",
    "deploy.only": "env-cmd -f .dev.vars wrangler deploy"
  },
  "dependencies": {
    "@iconify/vue": "^4.3.0",
    "@ionic/vue": "^8.4.3",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@pinia/nuxt": "^0.10.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.16",
    "@trpc/client": "^10.45.2",
    "@vueuse/core": "^12.7.0",
    "@vueuse/nuxt": "^12.7.0",
    "nitro-cloudflare-dev": "^0.2.1",
    "nuxt": "^3.15.4",
    "pinia": "^3.0.1",
    "sass": "^1.85.0",
    "superjson": "^2.2.2",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "trpc-nuxt": "^0.10.22",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "env-cmd": "^10.1.0",
    "wrangler": "^3.109.2"
  }
}