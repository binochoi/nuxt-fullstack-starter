{
  "name": "server",
  "private": true,
  "type": "module",
  "scripts": {
    "__ONLY__STAND_ALONE__::prepare": "run-s prepare.before; if-env NODE_ENV=prod && run-s prepare.prod || run-s prepare.dev; run-s prepare.after",
    "prepare.before": "dotenv -- run-s prepare.before.*",
    "prepare.before.cp-env": "rimraf .env; cp .env.example .env; cp .env.example .env.prod",
    "prepare.before.set-token": "dotenv -- printf \"INFISICAL_TOKEN=$BINOCORP_INFISICAL_TOKEN\n\" >> .env",
    "prepare.after": "dotenv -- run-s prepare.after.*",
    "prepare.after.notify": "echo end",
    "prepare.dev": "dotenv -- run-s prepare.dev.*",
    "prepare.dev.env.common": "dotenv -- infisical export --env=dev --path=/ >> .env",
    "prepare.dev.env.local": "dotenv -- infisical export --env=dev --path=/nuxt-fullstack-starter >> .env",
    "prepare.dev.mkcert": "mkcert localhost",
    "prepare.dev.nuxt": "dotenv -- nuxt prepare",
    "prepare.dev.nitro": "dotenv -- nitro prepare",
    "prepare.prod": "run-s prod.env.*",
    "prepare.prod.env.common": "dotenv -- infisical export --env=prod --path=/ >> .env.prod",
    "prepare.prod.env.local": "dotenv -- infisical export --env=prod --path=/nuxt-fullstack-starter >> .env.prod",
    "dev": "dotenv -- run-p typecheck dev.only",
    "dev.only": "dotenv -- nitro dev --port=$PORT --https --https.cert ./localhost.pem --https.key ./localhost-key.pem",
    "build": "run-s typecheck build.*",
    "build.only": "dotenv -- nitro build",
    "typecheck": "run-s prepare.nitro typecheck.only",
    "typecheck.only": "dotenv -- npx tsc --noEmit",
    "preview": "run-s preview.*",
    "preview.build": "dotenv -v ENV=preview -- nitro build",
    "preview.only": "dotenv -- pnpm preview.only.raw",
    "preview.only.raw": "wrangler dev --port=$PORT --https-key-path=./localhost-key.pem --https-cert-path=./localhost.pem --local-protocol https",
    "db:generate": "run-s db:generate.*",
    "db:generate.rm": "rimraf .cache/.migrations",
    "db:generate.better-auth": "dotenv -- npx @better-auth/cli generate --config ./src/auth.generator.ts --output ./src/database/schema/better-auth.schema.ts -y",
    "db:generate.drizzle": "dotenv -- drizzle-kit generate --config=./drizzle.config.ts",
    "db:migrate": "run-s db:generate db:migrate.*",
    "db:migrate.only": "dotenv -- drizzle-kit migrate --config=./drizzle.config.ts",
    "db:migrate.push": "dotenv -- drizzle-kit push",
    "deploy": "run-s prod.env deploy.*",
    "deploy.build": "dotenv -e .env.prod -- nitro build",
    "deploy.only": "dotenv -e .env.prod -- wrangler deploy"
  },
  "dependencies": {
    "api-to-rpc": "^0.1.1",
    "better-auth": "^1.3.2",
    "drizzle-orm": "^0.44.3",
    "nitro-cloudflare-dev": "^0.2.2",
    "nitro-rpc-definition": "^0.1.9",
    "postgres": "^3.4.7",
    "superjson": "^2.2.2",
    "nitropack": "latest"
  },
  "devDependencies": {
    "@primevue/nuxt-module": "^4.3.6",
    "@types/node": "^24.1.0",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "~0.31.4",
    "env-cmd": "^10.1.0",
    "rimraf": "^6.0.1",
    "wrangler": "^4.25.0"
  }
}